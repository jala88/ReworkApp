    <!-- FastClick -->
    <script src="~/page/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/page/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="~/page/vendors/Chart.js/dist/Chart.min.js"></script>
